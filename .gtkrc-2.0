# DO NOT EDIT! This file will be overwritten by LXAppearance.
# Any customization should be done in ~/.gtkrc-2.0.mine instead.

gtk-theme-name="oomox-arc-dark"
gtk-icon-theme-name="oomox-arc-dark"
gtk-font-name="Noto Sans, 12"
gtk-cursor-theme-name="Bibata-Modern-Ice"
gtk-cursor-theme-size=32
gtk-toolbar-style=GTK_TOOLBAR_BOTH_HORIZ
gtk-toolbar-icon-size=GTK_ICON_SIZE_LARGE_TOOLBAR
gtk-button-images=1
gtk-menu-images=1
gtk-enable-event-sounds=1
gtk-enable-input-feedback-sounds=1
gtk-xft-antialias=1
gtk-xft-hinting=1
gtk-xft-hintstyle="hintslight"
gtk-xft-rgba="rgb"
gtk-modules="canberra-gtk-module"

# Base16 oomox-arc-dark for gtk2 (https://github.com/dawikur/base16-gtk2)
# By oomox-arc-dark

gtk-color-scheme = "bg_color:#404552\nfg_color:#d3dae3\nbase_color:#404552\ntext_color:#d3dae3\nselected_bg_color:#443e58\nselected_fg_color:#d3dae3\ntooltip_bg_color:#404552\ntooltip_fg_color:#545966\ntitlebar_bg_color:#404552\ntitlebar_fg_color:#90a5f7\nmenubar_bg_color:#404552\nmenubar_fg_color:#90a5f7\ntoolbar_bg_color:#404552\ntoolbar_fg_color:#cca429\nmenu_bg_color:#404552\nmenu_fg_color:#d3dae3\npanel_bg_color:#404552\npanel_fg_color:#66bc56\nlink_color:#bea919"