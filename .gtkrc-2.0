# DO NOT EDIT! This file will be overwritten by LXAppearance.
# Any customization should be done in ~/.gtkrc-2.0.mine instead.

gtk-theme-name="oomox-Numix-Dark-Manjaro"
gtk-icon-theme-name="oomox-Numix-Dark-Manjaro"
gtk-font-name="Sans Regular 12"
gtk-cursor-theme-name="Bibata-Modern-Ice"
gtk-cursor-theme-size=32
gtk-toolbar-style=GTK_TOOLBAR_BOTH_HORIZ
gtk-toolbar-icon-size=GTK_ICON_SIZE_LARGE_TOOLBAR
gtk-button-images=1
gtk-menu-images=1
gtk-enable-event-sounds=1
gtk-enable-input-feedback-sounds=1
gtk-xft-antialias=1
gtk-xft-hinting=1
gtk-xft-hintstyle="hintslight"
gtk-xft-rgba="rgb"
gtk-modules="canberra-gtk-module"

# Base16 oomox-Materia-Dark-Manjaro for gtk2 (https://github.com/dawikur/base16-gtk2)
# By oomox-Materia-Dark-Manjaro

gtk-color-scheme = "bg_color:#303030\nfg_color:#fafafa\nbase_color:#303030\ntext_color:#fafafa\nselected_bg_color:#4a7d76\nselected_fg_color:#fafafa\ntooltip_bg_color:#303030\ntooltip_fg_color:#1c1c1c\ntitlebar_bg_color:#303030\ntitlebar_fg_color:#5c97c5\nmenubar_bg_color:#303030\nmenubar_fg_color:#5c97c5\ntoolbar_bg_color:#303030\ntoolbar_fg_color:#ccae4f\nmenu_bg_color:#303030\nmenu_fg_color:#fafafa\npanel_bg_color:#303030\npanel_fg_color:#369d6a\nlink_color:#e6c665"
