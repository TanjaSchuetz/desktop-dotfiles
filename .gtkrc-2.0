# DO NOT EDIT! This file will be overwritten by LXAppearance.
# Any customization should be done in ~/.gtkrc-2.0.mine instead.

include "/home/ralf/.gtkrc-2.0.mine"
gtk-theme-name="oomox-Materia-Dark-Orange"
gtk-icon-theme-name="oomox-Materia-Dark-Orange"
gtk-font-name="Sans Regular 12"
gtk-cursor-theme-name="Bibata-Modern-Ice"
gtk-cursor-theme-size=32
gtk-toolbar-style=GTK_TOOLBAR_BOTH_HORIZ
gtk-toolbar-icon-size=GTK_ICON_SIZE_LARGE_TOOLBAR
gtk-button-images=1
gtk-menu-images=1
gtk-enable-event-sounds=1
gtk-enable-input-feedback-sounds=1
gtk-xft-antialias=1
gtk-xft-hinting=1
gtk-xft-hintstyle="hintslight"
gtk-xft-rgba="rgb"
gtk-modules="canberra-gtk-module:gail:atk-bridge"

gtk-auto-mnemonics=true

gtk-color-scheme = "bg_color:#0c0a0a
color0:#0c0a0a
text_color:#eeeeee
selected_bg_color:#4c4945
selected_fg_color:#eeeeee
tooltip_bg_color:#0c0a0a
tooltip_fg_color:#eeeeee
titlebar_bg_color:#0c0a0a
titlebar_fg_color:#eeeeee
menu_bg_color:#0c0a0a
menu_fg_color:#eeeeee
link_color:#4c4945"
