{
    "cSpell.language": "de,de-DE,en,en-GB,en-US",
    "cSpell.allowCompoundWords": true,
    "diffEditor.codeLens": true,
    "editor.fontFamily": "monospace, Twemoji",
    "editor.fontSize": 16,
    "editor.fontWeight": "normal",
    "editor.tabSize": 2,
    "editor.renameOnType": true,
    "editor.renderControlCharacters": true,
    "files.autoSave": "onFocusChange",
    "files.watcherExclude": {
      "**/.git/objects/**": true,
      "**/.git/subtree-cache/**": true,
      "**/node_modules/*/**": true
    },
    "editor.suggestSelection": "first",
    "explorer.confirmDelete": false,
    "git.enableSmartCommit": true,
    "languageTool.language": "de",
    "merge-conflict.autoNavigateNextConflict.enabled": true,
    "python.languageServer": "Microsoft",
    "todo-tree.tree.showScanModeButton": false,
    "vscode-metalua-formatter.luaPath": "/usr/bin/lua5.1",
    "workbench.iconTheme": "eq-material-theme-icons-darker",
    "[lua]": {
      "editor.defaultFormatter": "trixnz.vscode-lua"
    },
    "lua.format.useTabs": null,
    "lua.targetVersion": "5.3",
    "[jsonc]": {
      "editor.defaultFormatter": "vscode.json-language-features"
    },
    "[typescript]": {
      "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "editor.fontLigatures": true,
    "git.confirmSync": false,
  }
